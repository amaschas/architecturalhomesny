{
  "dist": {
    "options": {
      "sourceMap": true,
      "nameCache": ".tmp/grunt-uglify-cache.json"
    },
    "files": [{
      "expand": true,
      "cwd": "static/js",
      "src": ["**/*.js", "!**/*.min.js"],
      "dest": "static/js",
      "ext": ".min.js",
      "extDot": "last"
    }]
  }
}
