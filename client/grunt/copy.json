			{ "src": "bower_components/truncate.js/truncate.js", "dest": "static/js/truncate.js"  },
			{ "src": "bower_components/history.js/scripts/bundled-uncompressed/html4+html5/jquery.history.js", "dest": "static/js/jquery.history.js"  },
			{ "src": "bower_components/the-modal/jquery.the-modal.js", "dest": "static/js/jquery.the-modal.js"  },
			{ "src": "bower_components/matchMedia/matchMedia.js", "dest": "static/js/matchMedia.js" },
			{ "src": "bower_components/slick.js/slick/slick.js", "dest": "static/js/slick.js" }
		]
	}
}