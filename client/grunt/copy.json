			{ "src": "bower_components/headroom.js/dist/headroom.js", "dest": "static/js/headroom.js"  },
			{ "src": "bower_components/jquery-breakpoints/jquery-breakpoints.js", "dest": "static/js/jquery-breakpoints.js"  },
			{ "src": "bower_components/jquery-history/scripts/jquery.history.js", "dest": "static/js/jquery.history.js"  },
			{ "src": "bower_components/jquery-touchswipe/jquery.touchSwipe.js", "dest": "static/js/jquery.touchSwipe.js"  },
			{ "src": "bower_components/jquery.cookie/jquery.cookie.js", "dest": "static/js/jquery.cookie.js"  },
			{ "src": "bower_components/jquery.simplemodal/src/jquery.simplemodal.js", "dest": "static/js/jquery.simplemodal.js"  },
			{ "src": "bower_components/picturefill/dist/picturefill.js", "dest": "static/js/picturefill.js"  },
